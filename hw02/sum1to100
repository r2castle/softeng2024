def isEven(n):
    if n % 2 == 0:
        return True
    else:
        return False
    
def main():
    total = 0

    # for i in range(1, 101): # 반복문
    #    if isEven(i):
    #       total += i
    
    total = sum([i for i in range(1, 101) if isEven(i)]) # 지능형리스트

    print(total)

if __name__ == "__main__":
    main()
